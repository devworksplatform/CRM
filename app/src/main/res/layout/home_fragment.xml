<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:gravity="center_horizontal|center_vertical"
		android:orientation="horizontal">
		<LinearLayout
			android:id="@+id/linear5"
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:padding="8dp"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="vertical">
			<ImageView
				android:id="@+id/imageview1"
				android:layout_width="25dp"
				android:layout_height="25dp"
				android:src="@drawable/logo"
				android:scaleType="fitCenter" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear6"
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:paddingTop="8dp"
			android:paddingRight="8dp"
			android:paddingBottom="8dp"
			android:gravity="left|center_vertical"
			android:orientation="vertical"
			android:layout_weight="1">
			<TextView
				android:id="@+id/textview1"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="Shopline"
				android:textSize="16sp"
				android:textColor="#212121" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear7"
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:padding="8dp"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="vertical">
			<ImageView
				android:id="@+id/imageview2"
				android:layout_width="18dp"
				android:layout_height="20dp"
				android:src="@drawable/search"
				android:scaleType="fitCenter" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear8"
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:padding="8dp"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="vertical">
			<ImageView
				android:id="@+id/imageview3"
				android:layout_width="29dp"
				android:layout_height="29dp"
				android:src="@drawable/menu"
				android:scaleType="fitCenter" />
		</LinearLayout>
	</LinearLayout>
	<androidx.core.widget.NestedScrollView
		android:id="@+id/linear2"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/linear3"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical">
			<LinearLayout
				android:visibility="gone"
				android:id="@+id/linear26"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:padding="8dp"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/linear27"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:orientation="vertical">
					<com.facebook.shimmer.ShimmerFrameLayout
						android:id="@+id/linear28"
						android:layout_width="100dp"
						android:layout_height="20dp"
						android:padding="8dp"
						android:background="#E0E0E0"
						android:orientation="horizontal" />
					<com.facebook.shimmer.ShimmerFrameLayout
						android:id="@+id/linear29"
						android:layout_width="150dp"
						android:layout_height="20dp"
						android:layout_marginTop="10dp"
						android:padding="8dp"
						android:background="#E0E0E0"
						android:orientation="horizontal" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear30"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="10dp"
					android:padding="8dp"
					android:orientation="horizontal">
					<com.facebook.shimmer.ShimmerFrameLayout
						android:id="@+id/linear31"
						android:layout_width="match_parent"
						android:layout_height="200dp"
						android:padding="8dp"
						android:background="#E0E0E0"
						android:orientation="horizontal" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear32"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="10dp"
					android:padding="8dp"
					android:orientation="vertical">
					<com.facebook.shimmer.ShimmerFrameLayout
						android:id="@+id/linear33"
						android:layout_width="120dp"
						android:layout_height="15dp"
						android:padding="8dp"
						android:background="#E0E0E0"
						android:orientation="horizontal" />
					<com.facebook.shimmer.ShimmerFrameLayout
						android:id="@+id/linear34"
						android:layout_width="match_parent"
						android:layout_height="20dp"
						android:layout_marginTop="10dp"
						android:padding="8dp"
						android:background="#E0E0E0"
						android:orientation="horizontal" />
					<com.facebook.shimmer.ShimmerFrameLayout
						android:id="@+id/linear35"
						android:layout_width="match_parent"
						android:layout_height="20dp"
						android:layout_marginTop="10dp"
						android:padding="8dp"
						android:background="#E0E0E0"
						android:orientation="horizontal" />
					<com.facebook.shimmer.ShimmerFrameLayout
						android:id="@+id/linear36"
						android:layout_width="200dp"
						android:layout_height="20dp"
						android:layout_marginTop="10dp"
						android:padding="8dp"
						android:background="#E0E0E0"
						android:orientation="horizontal" />
				</LinearLayout>

				<com.facebook.shimmer.ShimmerFrameLayout
					android:id="@+id/linear319"
					android:layout_width="match_parent"
					android:layout_height="200dp"
					android:background="#E0E0E0"
					android:orientation="horizontal"
					android:padding="8dp" />

				<LinearLayout
					android:id="@+id/linear37"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="20dp"
					android:padding="8dp"
					android:orientation="vertical">
					<com.facebook.shimmer.ShimmerFrameLayout
						android:id="@+id/linear38"
						android:layout_width="120dp"
						android:layout_height="15dp"
						android:padding="8dp"
						android:background="#E0E0E0"
						android:orientation="horizontal" />
					<com.facebook.shimmer.ShimmerFrameLayout
						android:id="@+id/linear39"
						android:layout_width="300dp"
						android:layout_height="20dp"
						android:layout_marginTop="10dp"
						android:padding="8dp"
						android:background="#E0E0E0"
						android:orientation="horizontal" />
					<com.facebook.shimmer.ShimmerFrameLayout
						android:id="@+id/linear40"
						android:layout_width="match_parent"
						android:layout_height="20dp"
						android:layout_marginTop="10dp"
						android:padding="8dp"
						android:background="#E0E0E0"
						android:orientation="horizontal" />
					<com.facebook.shimmer.ShimmerFrameLayout
						android:id="@+id/linear41"
						android:layout_width="match_parent"
						android:layout_height="20dp"
						android:layout_marginTop="10dp"
						android:padding="8dp"
						android:background="#E0E0E0"
						android:orientation="horizontal" />
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear4"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:paddingLeft="6dp"
				android:paddingRight="6dp"
				android:orientation="horizontal">
				<RelativeLayout
					android:id="@+id/linear9"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:orientation="vertical">
					<androidx.viewpager.widget.ViewPager
						android:id="@+id/viewpager_slider"
						android:layout_width="match_parent"
						android:layout_height="140dp" />
					<LinearLayout
						android:id="@+id/linear10"
						android:layout_width="match_parent"
						android:layout_height="140dp"
						android:gravity="center_horizontal|bottom"
						android:orientation="horizontal">
						<androidx.recyclerview.widget.RecyclerView
							android:id="@+id/recyclerview1"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_marginBottom="10dp" />
					</LinearLayout>
				</RelativeLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/totalCreditsLinear"
				android:orientation="vertical"
				android:layout_width="match_parent"
				android:layout_height="wrap_content">
				<androidx.cardview.widget.CardView
					xmlns:android="http://schemas.android.com/apk/res/android"
					xmlns:app="http://schemas.android.com/apk/res-auto"
					android:layout_width="match_parent"
					android:layout_height="130dp"
					android:layout_margin="16dp"
					app:cardCornerRadius="20dp"
					app:cardElevation="6dp"
					app:cardUseCompatPadding="true">
					<LinearLayout
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="vertical"
						android:layout_gravity="center"
						android:padding="20dp"
						android:background="@drawable/card_bg">
							<TextView
								android:id="@+id/tvBalanceLabel"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:text="Total Balance Credits"
								android:textSize="12sp"
								android:textColor="#ffffff"
								android:layout_marginBottom="4dp"/>
							<TextView
								android:id="@+id/tvBalanceAmount"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:elevation="20dp"
								android:text="..."
								android:textSize="32sp"
								android:textColor="#ffffff"
								android:textStyle="bold"
								android:textAppearance="?attr/textAppearanceHeadlineMedium" />
					</LinearLayout>
				</androidx.cardview.widget.CardView>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear11"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:orientation="horizontal">
				<TextView
					android:id="@+id/textview2"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:paddingRight="8dp"
					android:text="Categories"
					android:textSize="16sp"
					android:textColor="#212121"
					android:layout_weight="1" />
				<LinearLayout
					android:id="@+id/linear12"
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="vertical">
					<TextView
						android:id="@+id/textview3"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="View all"
						android:textSize="12sp"
						android:textColor="#757575" />
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear13"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:orientation="horizontal">
				<HorizontalScrollView
					android:id="@+id/hscroll1"
					android:layout_width="match_parent"
					android:layout_height="wrap_content">
					<LinearLayout
						android:id="@+id/linear16"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:orientation="horizontal">
						<LinearLayout
							android:id="@+id/All"
							android:layout_width="90dp"
							android:layout_height="32dp"
							android:background="#616B7C"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical">
							<TextView
								android:id="@+id/textview4"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:gravity="center_horizontal|center_vertical"
								android:text="All"
								android:textSize="12sp"
								android:textColor="#FFFFFF" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear17"
							android:layout_width="15dp"
							android:layout_height="match_parent"
							android:padding="8dp"
							android:orientation="vertical" />
						<LinearLayout
							android:id="@+id/Dress"
							android:layout_width="90dp"
							android:layout_height="32dp"
							android:background="#616B7C"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical">
							<TextView
								android:id="@+id/textview5"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:gravity="center_horizontal|center_vertical"
								android:text="Dress"
								android:textSize="12sp"
								android:textColor="#212121" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear19"
							android:layout_width="15dp"
							android:layout_height="match_parent"
							android:padding="8dp"
							android:orientation="vertical" />
						<LinearLayout
							android:id="@+id/accessories"
							android:layout_width="105dp"
							android:layout_height="32dp"
							android:background="#616B7C"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical">
							<TextView
								android:id="@+id/textview6"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:gravity="center_horizontal|center_vertical"
								android:text="Accessories"
								android:textSize="12sp"
								android:textColor="#212121" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear21"
							android:layout_width="15dp"
							android:layout_height="match_parent"
							android:padding="8dp"
							android:orientation="vertical" />
						<LinearLayout
							android:id="@+id/cardigan"
							android:layout_width="95dp"
							android:layout_height="32dp"
							android:background="#616B7C"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical">
							<TextView
								android:id="@+id/textview7"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:gravity="center_horizontal|center_vertical"
								android:text="Cardigan"
								android:textSize="12sp"
								android:textColor="#212121" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear23"
							android:layout_width="15dp"
							android:layout_height="match_parent"
							android:padding="8dp"
							android:orientation="vertical" />
						<LinearLayout
							android:id="@+id/sweater"
							android:layout_width="90dp"
							android:layout_height="32dp"
							android:background="#616B7C"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical">
							<TextView
								android:id="@+id/textview8"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:gravity="center_horizontal|center_vertical"
								android:text="Sweater"
								android:textSize="12sp"
								android:textColor="#212121" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear25"
							android:layout_width="15dp"
							android:layout_height="match_parent"
							android:padding="8dp"
							android:orientation="vertical" />
						<LinearLayout
							android:id="@+id/top"
							android:layout_width="90dp"
							android:layout_height="32dp"
							android:background="#616B7C"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical">
							<TextView
								android:id="@+id/textview9"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:gravity="center_horizontal|center_vertical"
								android:text="Top"
								android:textSize="12sp"
								android:textColor="#212121" />
						</LinearLayout>
					</LinearLayout>
				</HorizontalScrollView>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/load"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:orientation="horizontal">
				<ProgressBar
					android:id="@+id/progressbar1"
					android:layout_width="match_parent"
					android:layout_height="25dp"
					android:indeterminate="false"
					style="?android:progressBarStyle" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/sk"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="horizontal" />
		</LinearLayout>
	</androidx.core.widget.NestedScrollView>
</LinearLayout>
